Ext.define('Tramites.view.consultar.winTramiteViewController', {
    extend: 'Ext.app.ViewController',
    alias: 'controller.consultar.wintramite',

    onBtnEstadosConsultarClick: function(button, e, eOpts) {
        var winEstado = Ext.create('Tramites.view.consultar.winEstadoCons');
        winEstado.show();
    },

    onWindowAfterRender: function(component, eOpts) {
        var forma = Ext.getCmp('frmConsultarTramite').getForm();
        if(component.curRec){
            forma.loadRecord(component.curRec);
            if(component.curRec.data.TRA_RECIBIDO!==true)
            Ext.getCmp('btnEstadosConsultar').setDisabled(true);
        }
    }

});
