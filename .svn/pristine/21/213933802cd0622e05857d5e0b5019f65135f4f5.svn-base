                                    "codeClass": null,
                                    "userConfig": {
                                        "fn": "onBtnAgregarRolClick",
                                        "implHandler": [
                                            "var grid = Ext.getCmp('grdRol');",
                                            "var store = grid.getStore();",
                                            "var g = Ext.getCmp('grdUsuarioRol');",
                                            "var arrayKeys = g.getSelectionModel().selected.items;",
                                            "var indice = g.getSelectionModel().selectionStartIdx;",
                                            "if(arrayKeys.length === 0){",
                                            "    Ext.Msg.alert('Error', 'Debe escoger un usuario');",
                                            "}",
                                            "else",
                                            "{",
                                            "    var rec = g.selection;",
                                            "    var usuariot = rec.data.USR_USUARIO;",
                                            "    var strRol = Ext.getCmp('cbRoles').getStore();",
                                            "    strRol.proxy.extraParams = {",
                                            "        usuario: usuariot,",
                                            "        parametro : Ext.getCmp('cbParamRol').getValue()",
                                            "    };",
                                            "    store.insert(0,",
                                            "                 {",
                                            "        USR_USUARIO: usuariot,",
                                            "        ROL_ID: 0",
                                            "    });",
                                            "    strRol.load();",
                                            "}"
                                        ],
