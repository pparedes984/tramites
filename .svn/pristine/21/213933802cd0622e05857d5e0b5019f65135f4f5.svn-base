                                            "    var reco = context.record;",
                                            "    store.add(reco);",
                                            "    store.sync(",
                                            "        {",
                                            "          params:{",
                                            "              usuario: reco.data.USR_USUARIO,",
                                            "              parametro: Ext.getCmp('cbParamRol').getValue()",
                                            "          },",
                                            "          success: function(batch, success)",
                                            "          {",
                                            "            store.commitChanges();",
                                            "            store.load();",
                                            "          },",
                                            "",
