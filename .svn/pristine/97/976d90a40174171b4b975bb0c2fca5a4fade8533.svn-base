                    var nombres = formnombre.split('^');
                    var texto = '';
                    for(i=0;i<links.length;i++){
                        if(i===0)
                        texto = texto+'<a href="'+links[i]+'">'+nombres[i]+'</a>';
                        else
                        texto = texto+'<br>'+'<a href="'+links[i]+'">'+nombres[i]+'</a>';
                    }
