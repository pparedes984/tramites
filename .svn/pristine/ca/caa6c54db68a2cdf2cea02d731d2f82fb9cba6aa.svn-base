Ext.define('Tramites.view.panel.pnlNuevoViewController1', {
    extend: 'Ext.app.ViewController',
    alias: 'controller.panel.pnlconsultar',

    onGrdConsulta1Render: function(component, eOpts) {
        var strConsulta1 = Ext.getStore('Consulta1');
        strConsulta1.proxy.extraParams = {
            busqueda: 1,
            tramite: 0,
            parametro: 0
        };
        strConsulta1.load();
    },

    onButtonBuscarClick: function(button, e, eOpts) {
        var tramiteNum =  Ext.getCmp('txtTramiteId').getValue();
        var parametroNum = Ext.getCmp('cbParametroId').getValue();
        var strConsulta1 = Ext.getStore('Consulta1');
        strConsulta1.proxy.extraParams = {
            busqueda: 1,
            tramite: tramiteNum,
            parametro: parametroNum
        };
        strConsulta1.load();
    }

});
